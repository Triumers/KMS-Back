{
  "AWSEBDockerrunVersion": "1",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "giyeonlee/triumers-back",
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "${DB_HOST}"
        },
        {
          "name": "DB_PORT",
          "value": "${DB_PORT}"
        },
        {
          "name": "DB_NAME",
          "value": "${DB_NAME}"
        },
        {
          "name": "DB_USER",
          "value": "${DB_USER}"
        },
        {
          "name": "DB_PASSWORD",
          "value": "${DB_PASSWORD}"
        }
      ]
    }
  ]
}
